@page
@using System.Globalization
@model Inlämningsuppgift.Pages.AdminPages.adminmainModel
@{
   
}
<head>
    <link rel="stylesheet" href="~/css/style.css" />
</head>

<table class="table">
    <thead>
    <tr>
        <th>Namn</th>
        <th></th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model.CatName)
    {
        <tr>
            <td>@item.Namn</td>
           
